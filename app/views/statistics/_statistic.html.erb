<h2>
  <%= @title %>
  <%= link_to t(".download"), statistic_path(statistic:
    {from: @statistic.from, to: @statistic.to}, statistic_type: @statistic_type,
    format: "xls") if @objects.any? %>
</h2>

<% if @objects.any? %>
  <ul class="<%= @objects.first.class.name.downcase %>s">
    <%= render @objects %>
  </ul>
<% end %>
<%= paginate @objects, theme: "twitter-bootstrap-3" %>
